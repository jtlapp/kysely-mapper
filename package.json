{
  "name": "kysely-mapper",
  "version": "0.4.1",
  "author": "Joseph T. Lapp <arachnojoe@gmail.com>",
  "license": "MIT",
  "description": "Flexible Kysely-based utility for mapping between tables and objects",
  "keywords": [
    "kysely",
    "mapper",
    "orm",
    "object-relational mapping",
    "compiled",
    "cached",
    "parameterized",
    "parameters"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "/index.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "rm -rdf dist && tsc && tsc -p tsconfig.cjs.json",
    "docs:build": "typedoc --options ./typedoc.js ./src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "kysely": "^0.24.2",
    "kysely-params": "^0.3.10"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.3",
    "@types/jest": "^29.4.0",
    "better-sqlite3": "^8.1.0",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.24.5",
    "typescript": "^4.9.5"
  }
}
